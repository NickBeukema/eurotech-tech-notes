<p id="notice"><%= notice %></p>

<h1>Listing Repair Order Details</h1>

<table>
  <thead>
    <tr>
      <th>Cause</th>
      <th>Correction</th>
      <th>Parts</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @repair_order_details.each do |repair_order_detail| %>
      <tr>
        <td><%= repair_order_detail.cause %></td>
        <td><%= repair_order_detail.correction %></td>
        <td><%= repair_order_detail.parts %></td>
        <td><%= link_to 'Show', repair_order_detail %></td>
        <td><%= link_to 'Edit', edit_repair_order_detail_path(repair_order_detail) %></td>
        <td><%= link_to 'Destroy', repair_order_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

